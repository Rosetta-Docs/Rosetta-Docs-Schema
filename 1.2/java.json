{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://rosetta-docs.com/schema/1.2/java.json",
  "type": "object",
  "additionalProperties": false,
  "minProperties": 1,
  "properties": {
    "packages": {
      "type": "object",
      "minProperties": 1,
      "patternProperties": {
        "^": {
          "$ref": "java/package.json"
        }
      }
    },
    "types": {
      "type": "object",
      "description": "(Optional) A dictionary of unique integer-id type definitions to reference throughout the set.",
      "minProperties": 1,
      "patternProperties": {
        "^": {
          "anyOf": [
            {
              "$ref": "java/type.json"
            },
            {
              "type": "string"
            }
          ]
        }
      }
    }
  }
}
