{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://rosetta-docs.com/schema/1.2/java/constructor.json",
  "type": "object",
  "minProperties": 1,
  "additionalProperties": false,
  "properties": {
    "scope": {
      "type": "string",
      "enum": ["public", "package", "protected", "private"]
    },
    "deprecated": {
      "type": ["boolean", "string"],
      "description": "If the constructor is deprecated."
    },
    "parameters": {
      "type": "array",
      "minItems": 1,
      "additionalItems": false,
      "items": {
        "$ref": "parameter.json"
      }
    },
    "type_parameters": {
      "type": "array",
      "minItems": 1,
      "additionalItems": false,
      "items": {
        "$ref": "type_parameter.json"
      }
    },
    "notes": {
      "type": "string"
    }
  }
}
