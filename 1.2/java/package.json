{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://rosetta-docs.com/schema/1.2/java/package.json",
  "type": "object",
  "minItems": 1,
  "additionalProperties": false,
  "minProperties": 1,
  "properties": {
    "classes": {
      "patternProperties": {
        "^": {
          "$ref": "class.json"
        }
      }
    },
    "packages": {
      "patternProperties": {
        "^": {
          "$ref": "package.json"
        }
      }
    },
    "notes": {
      "type": "string"
    },
    "tags": {
      "$ref": "../tags.json"
    }
  }
}
