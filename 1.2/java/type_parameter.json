{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://rosetta-docs.com/schema/1.2/java/type_parameter.json",
  "type": "object",
  "additionalProperties": false,
  "minProperties": 1,
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "anyOf": [
        {
          "$ref": "type.json"
        },
        {
          "type": "string"
        },
        {
          "type": "integer"
        }
      ]
    },
    "notes": {
      "type": "string"
    }
  },
  "required": ["name", "type"]
}
